APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Keycloak Guard
KEYCLOAK_REALM_PUBLIC_
KEYCLOAK_LOAD_USER_FROM_DATABASE=false
KEYCLOAK_APPEND_DECODED_TOKEN=true
KEYCLOAK_ALLOWED_RESOURCES=subscriber-app
KEYCLOAK_LEEWAY=60

# DB not required for this lab (in-memory demo). If you want DB, fill below and switch flags.
DB_CONNECTION=sqlite
DB_DATABASE=:memory:
